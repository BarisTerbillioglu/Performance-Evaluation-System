VITE_ENVIRONMENT=staging

# Analytics and Monitoring
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/staging-project-id
VITE_GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID_STAGING
